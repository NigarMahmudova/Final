


    <!-- Main section start -->
    <div id="fx-page-outer">
        <main id="fx-page-content">

            <div class="container">
                <div class="fx-login-content">
                    <div class="login-content-wrapper">
                        <span class="login-label">Reset Password</span>
                        <form id="reset-password-part" method="POST" aria-labelledby="ui-id-3">
                            <div class="login-fields">
                                <label style="margin-bottom: 1rem;">Lost your password? Please enter your email address. You will receive
                                    a link
                                    to create a new password via email.
                                </label>
